# OpenAI API Key (如果使用OpenAI作为提供商)
# OPENAI_API_KEY=your_openai_api_key_here

# 为了兼容性，将OpenRouter API Key也设置为OPENAI_API_KEY
# 这样ChatOpenAI可以正确使用OpenRouter API
OPENAI_API_KEY=your_openrouter_api_key_here


# TradingAgents 环境变量配置文件
# 复制此文件为 .env 并填入您的API密钥

# =============================================================================
# LLM API 配置
# =============================================================================

# OpenRouter API (推荐主要使用)
# 获取地址: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Groq API (高速推理)
# 获取地址: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Together AI API (开源模型丰富)
# 获取地址: https://api.together.xyz/settings/api-keys
TOGETHER_API_KEY=your_together_api_key_here

# Hugging Face API (社区模型)
# 获取地址: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Google AI Studio API (Gemini模型)
# 获取地址: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# =============================================================================
# 本地模型配置
# =============================================================================

# Ollama 本地服务地址
OLLAMA_BASE_URL=http://localhost:11434

# =============================================================================
# 金融数据源API配置
# =============================================================================

# AKShare Token (A股数据)
# 获取地址: https://www.akshare.xyz/
AKSHARE_TOKEN=your_akshare_token_here

# 聚宽JQData (A股数据增强)
# 获取地址: https://www.joinquant.com/default/index/sdk
JQDATA_USERNAME=your_jqdata_username_here
JQDATA_PASSWORD=your_jqdata_password_here

# Tushare Token (A股数据增强)
# 获取地址: https://tushare.pro/register
TUSHARE_TOKEN=57d7083719b1ed4c944b23b6a08745ffbb892306c26e8523d41f5675

# Alltick Token (A股实时数据)
# 获取地址: https://alltick.co/
ALLTICK_TOKEN=your_alltick_token_here

# Finnhub API Key (美股数据)
# 获取地址: https://finnhub.io/register
FINNHUB_API_KEY=your_finnhub_api_key_here

# Alpha Vantage API Key (金融数据)
# 获取地址: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Yahoo Finance (通常不需要API Key)
# YAHOO_FINANCE_API_KEY=optional

# =============================================================================
# 新闻和社交媒体API配置
# =============================================================================

# NewsAPI Key (新闻数据)
# 获取地址: https://newsapi.org/register
NEWS_API_KEY=your_news_api_key_here

# Reddit API (社交媒体数据)
# 获取地址: https://www.reddit.com/prefs/apps
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here
REDDIT_USER_AGENT=TradingAgents/1.0

# Twitter API (可选)
# 获取地址: https://developer.twitter.com/
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# =============================================================================
# 数据库配置
# =============================================================================

# 数据库连接字符串 (可选，用于数据持久化)
DATABASE_URL=sqlite:///tradingagents.db

# Redis 配置 (可选，用于缓存)
REDIS_URL=redis://localhost:6379/0

# =============================================================================
# 应用配置
# =============================================================================

# 应用环境 (development, production, testing)
ENVIRONMENT=development

# 日志级别 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# 结果输出目录
RESULTS_DIR=eval_results

# 是否启用在线工具 (true/false)
ONLINE_TOOLS=true

# 最大辩论轮数
MAX_DEBATE_ROUNDS=1

# 最大风险讨论轮数
MAX_RISK_DISCUSS_ROUNDS=1

# =============================================================================
# 安全配置
# =============================================================================

# 加密密钥 (用于敏感数据加密)
SECRET_KEY=your_secret_key_here

# JWT密钥 (如果使用Web界面)
JWT_SECRET=your_jwt_secret_here

# =============================================================================
# 代理配置 (可选)
# =============================================================================

# HTTP代理 (如果需要)
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=https://proxy.example.com:8080

# =============================================================================
# 监控和分析配置 (可选)
# =============================================================================

# Langfuse 配置 (LLM监控)
# LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
# LANGFUSE_SECRET_KEY=your_langfuse_secret_key
# LANGFUSE_HOST=https://cloud.langfuse.com

# Weights & Biases 配置 (实验跟踪)
# WANDB_API_KEY=your_wandb_api_key
# WANDB_PROJECT=tradingagents
